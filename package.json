{
  "name": "remark-mdx-react-docgen-typescript",
  "version": "0.1.0",
  "description": "remark plugin to support react-docgen-typescript in MDX",
  "license": "Apache-2.0",
  "repository": "Rippling/remark-mdx-react-docgen-typescript",
  "bugs": "https://github.com/Rippling/remark-mdx-react-docgen-typescript/issues",
  "author": "Divyansh Kumar <mr.divyanshindore@gmail.com>",
  "keywords": [
    "markdown",
    "react-docgen-typescript",
    "mdast",
    "plugin",
    "remark",
    "remark-plugin",
    "unified"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "prebuild": "npm run clean",
    "release": "standard-version",
    "test": "npm run build && vitest",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "esast-util-from-js": "^2.0.1",
    "mdast": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.0.0",
    "react-docgen-typescript": "^2.2.2",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "mdast-util-directive": "^3.0.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "remark-cli": "^12.0.0",
    "remark-directive": "^3.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vfile": "^6.0.1",
    "vitest": "^1.2.2"
  }
}
